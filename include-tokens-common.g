(:  these are the tokens used by both the flg lexer generator and fpg parser generator.
    if these are rearranged, both flg and fpg must be rebuilt.  :)
%tokens (
  C_TOKEN,
  C_DIRECTIVE,
  C_EQUALS,
  C_NUMBER,
  C_POS_PARAM,
  C_QUOTEDSTR,
  C_PAREN_OPEN,
  C_PAREN_CLOSE,
  C_COMMA,
  C_PLUS,
  C_MINUS,
  C_TIMES,
  C_DIVIDE,
  C_MODULO,
  C_DERIVES,
  C_ACTION,
)
